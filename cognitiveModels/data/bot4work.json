    {
      "pattern": "(receive|get|read) [the] {Weather.WeatherCondition} (warning[s]|alert[s]) [for] {datetimeV2}",
      "intent": "l_Weather"
    },
    {
      "pattern": "[the] {Weather.WeatherRange} {Weather.Historic} {Weather.Historic} (in|at) {geographyV2}",
      "intent": "l_Weather"
    },
    {
      "pattern": "(what is|what's) the {Weather.WeatherCondition} of {geographyV2} (in|at) {temperature}",
      "intent": "l_Weather"
    },
    {
      "pattern": "(is|are) [(the|my)] {HomeAutomation.DeviceType} [{HomeAutomation.Setting}] in [(the|my)] [{HomeAutomation.Location}] [set] [to] {HomeAutomation.Setting}",
      "intent": "l_HomeAutomation"
    },
    {
      "pattern": "do[es] (the|those) (device[s]|{HomeAutomation.DeviceType}) [{HomeAutomation.Setting}][?]",
      "intent": "l_HomeAutomation"
    },
    {
      "pattern": "[(which mode|what's)] is (the|my) ({HomeAutomation.DeviceType}|{HomeAutomation.SettingType}) set to",
      "intent": "l_HomeAutomation"
    },
    {
      "pattern": "start up [the] {HomeAutomation.DeviceType} [in {HomeAutomation.Location}]",
      "intent": "l_HomeAutomation"
    },
    {
      "pattern": "(are|is|do[es]) [(the|my)] [{HomeAutomation.Location}] {HomeAutomation.DeviceType} [{HomeAutomation.SettingType}] {HomeAutomation.Setting} [or {HomeAutomation.Setting}]",
      "intent": "l_HomeAutomation"
    },
    {
      "pattern": "make ({HomeAutomation.DeviceName}|{HomeAutomation.DeviceType}) [in {HomeAutomation.Location}] turned on",
      "intent": "l_HomeAutomation"
    },
    {
      "pattern": "[please] stop the {HomeAutomation.DeviceType} [for me]",
      "intent": "l_HomeAutomation"
    }
